// Zed settings
//
// For information on how to configure Zed, see the Zed
// documentation: https://zed.dev/docs/configuring-zed
//
// To see all of Zed's default settings without changing your
// custom settings, run `zed: open default settings` from the
// command palette (cmd-shift-p / ctrl-shift-p)
{
  "language_models": {
    "openai_compatible": {
      "a4f": {
        "api_url": "https://api.a4f.co/v1",
        "available_models": [
          {
            "name": "provider-5/gpt-5.1-2025-11-13",
            "max_tokens": 200000,
            "max_output_tokens": 32000,
            "max_completion_tokens": 200000,
            "capabilities": {
              "tools": true,
              "images": true,
              "parallel_tool_calls": false,
              "prompt_cache_key": false
            }
          }
        ]
      },
      "Mint": {
        "api_url": "https://x666.me/v1",
        "available_models": [
          {
            "name": "gemini-2.5-pro",
            "max_tokens": 200000,
            "max_output_tokens": 32000,
            "max_completion_tokens": 200000,
            "capabilities": {
              "tools": true,
              "images": true,
              "parallel_tool_calls": true,
              "prompt_cache_key": true
            }
          }
        ]
      },
      "test": {
        "api_url": "test",
        "available_models": [
          {
            "name": "test",
            "max_tokens": 200000,
            "max_output_tokens": 32000,
            "max_completion_tokens": 200000,
            "capabilities": {
              "tools": true,
              "images": true,
              "parallel_tool_calls": true,
              "prompt_cache_key": true
            }
          }
        ]
      }
    }
  },
  "context_servers": {
    "mcp-server-context7": {
      "enabled": true,
      "settings": {}
    }
  },
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_text_threads": false
  },
  "show_edit_predictions": false,
  "languages": {
    "Plain Text": {
      "show_edit_predictions": false
    }
  },
  "agent": {
    "default_profile": "minimal",
    "default_model": {
      "provider": "a4f",
      "model": "provider-5/gpt-5.1-2025-11-13"
    },
    "model_parameters": [],
    "inline_assistant_model": {
      "provider": "google",
      "model": "gemini-2.5-flash"
    }
  },
  "base_keymap": "VSCode",
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "soft_wrap": "editor_width",
  "theme": {
    "mode": "system",
    "light": "Ayu Dark",
    "dark": "One Dark"
  }
}
